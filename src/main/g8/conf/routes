# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /health                                               controllers.Health.internalCheck()
GET     /$name$/health                                        controllers.Health.check(cached: Boolean ?= true)
GET     /$name$/admin/conf                                    controllers.Admin.conf()
GET     /$name$/swagger.json                                  controllers.ApiHelpController.getResources
